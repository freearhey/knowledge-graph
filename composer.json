{
    "name": "freearhey/knowledge-graph",
    "description": "A simple PHP client for working with Google Knowledge Graph API",
    "keywords": ["google", "knowledge", "graph", "api", "client", "php"],
    "homepage": "https://github.com/freearhey/knowledge-graph",
    "version": "2.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arhey",
            "email": "free.arhey@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "tightenco/collect": "5.6.35",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "KnowledgeGraph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    }
}
